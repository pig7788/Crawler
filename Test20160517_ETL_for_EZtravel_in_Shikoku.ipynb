      "                        <div class=\"event-box\">\n",
      "                            <% _.each(day.events, function(event, i) { %>\n",
      "                                <div class=\"eventDate\" data-date=\"<%= event.date %>\"></div>\n",
      "                                <div class=\"eventStatus<%= i %>\" data-status=\"<%= event.fullStatus %>\"></div>\n",
      "                                <div class=\"vendNo\" data-vendNo=\"<%= event.vendNo %>\"></div>\n",
      "                                <% if(event.vendNo) { %>\n",
      "                                    <div class=\"eventLink<%= i %>\" data-link=\"<%= event.prodUrl %><%= event.vendNo %>/<%= event.prodNo %>\"></div>\n",
      "                                <% }else{ %>\n",
      "                                    <div class=\"eventLink<%= i %>\" data-link=\"<%= event.prodUrl %>\"></div>\n",
      "                                <% } %>\n",
      "                                <div class=\"availableNum<%= i %>\" data-num=\"<%= event.allot %>\"></div>\n",
      "                                <div class=\"promoText<%= i %>\" data-promo=\"<%= event.promoText %>\"></div>\n",
      "                                <div class=\"eventPrice<%= i %>\" data-price=\"<%= event.price %>\"></div>\n",
      "                                <% if(event.fullStatus == '暫滿') { %>\n",
      "                                    <div class=\"event-status text-dark-gray full-text\"><%= event.fullStatus %></div>\n",
      "                                <% } else if(event.fullStatus == '結團') { %>\n",
      "                                    <div class=\"event-status text-dark-gray full-text\"><%= event.fullStatus %></div>\n",
      "                                <% } else { %>\n",
      "                                    <div class=\"event-status nofull-text\">\n",
      "                                        <span class=\"text-orange clndr-price\"><%= event.price %></span><span class=\"text-orange\">起</span>\n",
